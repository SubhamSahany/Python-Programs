   "source": [
    "### WAP to print the factorial of the given number, where number is entered by the user"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "ffd3818d",
   "metadata": {},
   "outputs": [],
   "source": [
    "def factorial(n):\n",
    "    if (n==1 or n==0):\n",
    "        return 1\n",
    "    else:\n",
    "        return n*factorial(n-1)\n",
    "\n",
    "num = int(input(\"Enter a number: \"))\n",
    "print(\"Factorial of \",num,\"is:\",factorial(num))   "
   ]
  }
