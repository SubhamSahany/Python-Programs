   "source": [
    "### WAP to print the sum of n natural numbers, where n is entered by the user"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "5a95b5c8",
   "metadata": {},
   "outputs": [],
   "source": [
    "n = int(input(\"Enter a number: \"))\n",
    "sum = 0\n",
    "while(n>0):\n",
    "    sum = sum + n\n",
    "    n = n-1\n",
    "print(\"The sum of n natural numbers is \",sum)  "
   ]
  }
